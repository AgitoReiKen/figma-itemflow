{
  "compilerOptions": {
    "target": "es6",
    "lib": [
      "es6",
      "DOM"
    ],
    "typeRoots": [
      "./node_modules/@types",
      "./node_modules/@figma"
    ],
    "module": "commonjs",
    "moduleResolution": "node",
    "baseUrl": "src",
    "allowJs": false,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
  }
}